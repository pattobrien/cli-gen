  "version": "0.2.0",
  "configurations": [
    {
      "name": "cli-gen",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "example (build runner)",
      "cwd": "example",
      "program": ".dart_tool/build/entrypoint/build.dart",
      "request": "launch",
      "type": "dart",
      "vmAdditionalArgs": ["--observe", "--pause-isolates-on-exit"],
      "args": ["build", "--delete-conflicting-outputs"]
    },
    {
      "name": "cli_annotations",
      "cwd": "src/cli_annotations",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "cli_gen",
      "cwd": "src/cli_gen",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "example_project",
      "cwd": "src/cli_gen/test/example_project",
      "request": "launch",
      "type": "dart"
    }
  ]
}
